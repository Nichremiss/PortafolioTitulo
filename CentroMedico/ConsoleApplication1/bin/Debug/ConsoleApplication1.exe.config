<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IPrevision" />
                <binding name="BasicHttpBinding_IMedico" />
                <binding name="BasicHttpBinding_IEspecialidad" />
                <binding name="BasicHttpBinding_IAgendaDiariasvc" />
                <binding name="BasicHttpBinding_ISucursales" />
                <binding name="BasicHttpBinding_IReserva" />
                <binding name="BasicHttpBinding_IReserva1" />
                <binding name="BasicHttpBinding_IPaciente" />
                <binding name="BasicHttpBinding_ISecretaria" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:59231/Prevision.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IPrevision" contract="PrevisionSvc.IPrevision"
                name="BasicHttpBinding_IPrevision" />
            <endpoint address="http://localhost:59231/Medico.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IMedico" contract="Medico.IMedico"
                name="BasicHttpBinding_IMedico" />
            <endpoint address="http://localhost:59231/Especialidad.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IEspecialidad" contract="Especialidad.IEspecialidad"
                name="BasicHttpBinding_IEspecialidad" />
            <endpoint address="http://localhost:59231/AgendaDiariasvc.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IAgendaDiariasvc"
                contract="AgendaDiaria.IAgendaDiariasvc" name="BasicHttpBinding_IAgendaDiariasvc" />
            <endpoint address="http://localhost:59231/Sucursales.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_ISucursales" contract="SucursalSvc.ISucursales"
                name="BasicHttpBinding_ISucursales" />
            <endpoint address="http://localhost:59231/Reserva.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IReserva" contract="ReservaSvc.IReserva"
                name="BasicHttpBinding_IReserva" />
            <endpoint address="http://localhost:59231/Reserva.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IReserva1" contract="Reserva.IReserva"
                name="BasicHttpBinding_IReserva1" />
            <endpoint address="http://localhost:59231/Paciente.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IPaciente" contract="PacienteSvc.IPaciente"
                name="BasicHttpBinding_IPaciente" />
            <endpoint address="http://localhost:59231/Secretaria.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_ISecretaria" contract="SecretariaSvc.ISecretaria"
                name="BasicHttpBinding_ISecretaria" />
        </client>
    </system.serviceModel>
</configuration>